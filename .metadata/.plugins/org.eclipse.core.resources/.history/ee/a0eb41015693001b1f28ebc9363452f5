package com.javalec.base;

public class Main {

	public static void main(String[] args) {
		// 초기 값이 없는 배열 선언 및 데이터 추가하기 Array
		
		String[] str = new String[7];
		
		str[0] ="일요일";
		str[1] ="월요일";
		str[2] ="화요일";
		str[3] ="수요일";
		str[4] ="목요일";
		str[5] ="금요일";
		str[6] ="토요일";
		
		System.out.println(str);
		System.out.println(str[0] + " , " + str[3] + " , "+ str[5]);
		
		

	}

}
