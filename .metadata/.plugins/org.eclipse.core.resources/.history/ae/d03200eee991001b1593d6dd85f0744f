package com.javalec.base;

import java.util.Scanner;

public class Main2 {

	public static void main(String[] args) {
		Scanner scanner =  new Scanner(System.in);
		int num = scanner.nextInt();
		int factorial = 0;
		
		for(int i=1; i<=num; i--);{
			factorial = factorial * i;
		}
		System.out.println(factorial);		
	}

}
